
@{
    ViewBag.Title = "Update";
}

<input type="hidden" id="hd_path" value="@ViewBag.path" />

<h2>Update</h2>

@*<script src="~/Scripts/jquery-3.4.1.js"></script>
<script src="~/Scripts/jquery-3.4.1.min.js"></script>*@

<form method="post">
    <div class="row mb-3">
        <label class="col-sm-3 col-form-label">id</label>
        <div class="col-sm-6">
            <input type="text" class="form-control" name="id" id="id" value="@Session["updateid"].ToString()" readonly="readonly">
        </div>
    </div>
    <div class="row mb-3">
        <label class="col-sm-3 col-form-label">Pekerjaan</label>
        <div class="col-sm-6">
            <input type="text" class="form-control" name="nama_pekerjaan" id="nama_pekerjaan">
        </div>
    </div>
</form>
<button type="button" class="btn btn-primary" onclick="Update();">Edit</button>
<button class="btn btn-danger" data-toggle="modal" onclick="location.href='@Url.Action("Get", "Pekerjaan")'">Back</button>
