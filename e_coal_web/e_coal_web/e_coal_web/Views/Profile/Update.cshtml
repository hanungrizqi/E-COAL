
@{
    ViewBag.Title = "Update";
}

<h2>Update</h2>

<script src="~/Scripts/jquery-3.4.1.js"></script>
<script src="~/Scripts/jquery-3.4.1.min.js"></script>

<form method="post">
    <div class="row mb-3">
        <label class="col-sm-3 col-form-label">id</label>
        <div class="col-sm-6">
            <input type="text" class="form-control" name="id" id="id" value="@Session["updateid"].ToString()" readonly="readonly">
        </div>
    </div>
    <div class="row mb-3">
        <label class="col-sm-3 col-form-label">Profile</label>
        <div class="col-sm-6">
            <input type="text" class="form-control" name="profile" id="profile">
        </div>
    </div>
</form>
<button type="button" class="btn btn-primary" onclick="Update();">Edit</button>
<button class="btn btn-danger" data-toggle="modal" onclick="location.href='@Url.Action("Get", "Profile")'">Back</button>
